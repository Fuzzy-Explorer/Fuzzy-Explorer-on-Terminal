alt-g:execute(echo 1 >| ~/.fzt_explorer/.status/.goto.status)+abort
alt-i:toggle-preview
alt-b:preview:echo {} | cut -f 2 -d ' ' | xargs -rI{a} sh -c 'if [ -f {a} ]; then ls -ldhG {a}; echo; richcat {a} -w 30; else ls -ldhG {a}; echo; lsi {a}; fi'
