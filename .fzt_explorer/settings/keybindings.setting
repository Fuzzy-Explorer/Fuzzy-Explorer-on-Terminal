bind-key ESC:_fzte_functions_interrupt
bind-key alt-c:_fzte_functions_interrupt
bind-key ctrl-c:_fzte_functions_interrupt
bind-key left:_fzte_functions_cd_above
bind-key alt-h:_fzte_functions_cd_above
bind-key backward-eof:_fzte_functions_cd_above
bind-key alt-k:_fzte_functions_cursor_up
